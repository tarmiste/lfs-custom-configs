############# Building package
cd $UNPACKDIR

patch -p1 -i ../qupzilla-2.1.2-openssl1.1-1.patch &&

export QUPZILLA_PREFIX=/usr &&
qmake                       &&
make
make -j1 install
xdg-icon-resource forceupdate --theme hicolor &&
update-desktop-database -q
ldconfig

cd $BUILD_DIR
[[ -n "$KEEP_FILES" ]] || rm -rf $UNPACKDIR unpacked

exit