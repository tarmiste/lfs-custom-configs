############# Building package
cd $UNPACKDIR

./configure --prefix=/usr     \
            --sysconfdir=/etc \
            --disable-static  &&
make
#make -k check || true
make -j1 install
mkdir -vp /usr/share/doc/libnl-3.3.0 &&
tar -xf ../libnl-doc-3.3.0.tar.gz --strip-components=1 --no-same-owner \
    -C  /usr/share/doc/libnl-3.3.0
ldconfig

cd $BUILD_DIR
[[ -n "$KEEP_FILES" ]] || rm -rf $UNPACKDIR unpacked

exit