############# Building package
cd $UNPACKDIR

./configure --prefix=/usr           \
            --with-tcl=/usr/lib     \
            --enable-shared         \
            --mandir=/usr/share/man \
            --with-tclinclude=/usr/include &&
make
#make -k test || true
make -j1 install &&
ln -svf expect5.45/libexpect5.45.so /usr/lib
ldconfig

cd $BUILD_DIR
[[ -n "$KEEP_FILES" ]] || rm -rf $UNPACKDIR unpacked

exit