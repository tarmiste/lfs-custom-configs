############# Building package
cd $UNPACKDIR

./configure --prefix=/usr --disable-static &&
make
make html
#make -k check || true
make -j1 install
mkdir                   /usr/share/doc/gsl-2.4 &&
cp -R doc/_build/html/* /usr/share/doc/gsl-2.4
ldconfig

cd $BUILD_DIR
[[ -n "$KEEP_FILES" ]] || rm -rf $UNPACKDIR unpacked

exit